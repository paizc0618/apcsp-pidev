#include <stdio.h>
#include <string.h>

struct Student
{
  char first[50];
  char last[50];
  int age;
  int studentid;
};

void printStudent(struct Student* student)
{

  strcpy(student.first, "Collin");
  strcpy(student.last, "Paiz");
  student.age = 18;
  student.studentid = 840618
  printf("Name: %s %s\n", student->first, student->last);
  printf("Age: %d\n", student->age);
  printf("StudentID: %d\n", student->studentid);
}

int main()
{
  printStudent(struct Student* student);

}
